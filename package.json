    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.11",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"
  },
  "resolutions": {
    "debug": "4.3.7",
    "underscore": "1.13.7",
    "string-width": "4.2.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@1.22.19"
}
